#include <unistd.h>

void	ft_putchar(char c)
{
	write(1, &c, 1);
}

char	ft_putstr(char *str)
{
	int		counter;

	counter = 0;
	while (str[counter] != '\0')
	{
		ft_putchar(str[counter]);
		counter++;
	}
}

int		main(int argc,char **argv)
{
	int		i;

	if (argc == 1)
	{
		return (0);
	}

	i = 1;
	while (i <= argc)
	{
		ft_putstr(argv[i]);
		i++;
	}
	ft_putchar('\n');
	return (0);
}
